.divLine {
  background: #e0e3da;
  width: 100%;
  height: 15rpx;
}

.divLine2 {
  background: rgb(255, 255, 255);
  width: 100%;
  height: 15rpx;
}

.divLine3 {
  background: #e0e3da;
  width: 100%;
  height: 1rpx;
}

.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #ccc;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
}

.swiper-tab-item {
  width: 30%;
  color: #434343;
  font-size: 20px;
}

.active {
  color: #f65959;
  border-bottom: 4rpx solid #f65959;
}

/* swiper{

} */

.orders-container {
  height: 40px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 30px 10px;
}

.line-container {
  height: 2rpx;
  background-color: #353535;
  width: 98%;
}

.background {
  display: flex;
  width: 20px;
  height: 20px;
  align-items: center;
  background-image: url("data:image/jpeg;base64,");
  background-size: 100% 100%;
}

.send-status-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 15px;
  color: red;
  padding: 0 10px
}

.status-container {
  text-align: right;
  font-size: 15px;
  color: red;
  padding: 5px 10px 0 0;
}

.button-container {
  width: 150px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

button {
  background: white;
  border: 1px solid #c33;
  border-radius: 10px;
}

.right-container {
  height: 94px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.message-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.sum-container {
  font-size: 15px;
  color: #cfcfcf;
}

swiper {
  height: 100%;
}
